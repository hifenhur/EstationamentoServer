<ul class="nav nav-tabs">
  <li class=active> <%= link_to tag_icon(:list, pat(:list)), url(:parking_cost_histories, :index) %></li>
</ul>
<div class=tabs-content>
  <table id=list class="table table-striped table-hover table-condensed">
    <thead>
      <tr>
                <th class=header> <%= mat(:parking_cost_history, :id) %></th>
                <th class=header> <%= mat(:parking_cost_history, :date) %></th>
                <th class=header> <%= mat(:parking_cost_history, :parking_cost) %></th>
                <th class=header> <%= mat(:parking_cost_history, :card_type) %></th>
    </thead>

    <tbody>
      <% @parking_cost_histories.each do |parking_cost_history| %>
        <tr class=list-row>
          <td class=list-column> <%= parking_cost_history.id %> </td>
          <td class=list-column> <%= parking_cost_history.date %> </td>
          <td class=list-column> <%= parking_cost_history.parking_cost.round(2) %> </td>
          <td class=list-column> <%= parking_cost_history.card_type %> </td>
      <% end %>
    </tbody>
  </table>

  <%= will_paginate @parking_cost_histories %>
</div>