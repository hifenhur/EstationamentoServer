<ul class="nav nav-tabs">
  <li class=active> <%= link_to tag_icon(:list, pat(:list)), url(:parking_histories, :index) %></li>
</ul>
<div class=tabs-content>
  <table id=list class="table table-striped table-hover table-condensed">
    <thead>
      <tr>
                <th class=header> <%= mat(:parking_history, :id) %></th>
                <th class=header> <%= mat(:parking_history, :date_time_reg) %></th>
                <th class=header> <%= mat(:parking_history, :card_id) %></th>
                <th class=header> <%= mat(:parking_history, :dt_time_in) %></th>
                <th class=header> <%= mat(:parking_history, :dt_time_out) %></th>
    </thead>

    <tbody>
      <% @parking_histories.each do |parking_history| %>
        <tr class=list-row>
          <td class=list-column> <%= parking_history.id %> </td>
          <td class=list-column> <%= l parking_history.date_time_reg, format: :short if parking_history.date_time_reg %> </td>
          <td class=list-column> <%= link_to parking_history.card_id.to_s, "enroll_cards/edit/#{parking_history.card_id}" %> 
          </td>
          <td class=list-column> <%= l parking_history.dt_time_in, format: :short if parking_history.dt_time_in %> </td>
          <td class=list-column> <%= l parking_history.dt_time_out, format: :short if parking_history.dt_time_out %> </td>
          
      <% end %>
    </tbody>
  </table>
  <%= will_paginate @parking_histories %>
</div>